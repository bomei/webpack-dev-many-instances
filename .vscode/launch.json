  "version": "0.2.0",
  "configurations": [

    {
      "name":"Server.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/server.js"
    },
    {
      "name":"Dev-middleware",
      "type":"node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/test/test-dev-middleware.js"
    },
    {
      "name": "wrapper",
      "type":"node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/wp-dev-middleware-dynamic-config/test/test-wrapper.js",
    },
    {
      "name": "configStore",
      "type":"node",
      "request": "launch",
      "program": "${workspaceFolder}/raw/wp-dev-middleware-dynamic-config/test/test-config-store.js",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/dist/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "program": "${file}",
      "sourceMaps": true,
      "runtimeExecutable": "babel-node",
      "runtimeArgs": [
        "--nolazy"
      ],
    }
    
  ]
}
